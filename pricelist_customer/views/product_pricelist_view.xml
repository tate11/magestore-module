<odoo>
    <data>
        <record id="product_pricelist_view_inherits_ben" model="ir.ui.view">
            <field name="name">product.pricelist.form.inherits.ben</field>
            <field name="model">product.pricelist</field>
            <field name="inherit_id" ref="product.product_pricelist_view"/>
            <field name="arch" type="xml">
                <xpath expr="//group//field[@name='country_group_ids']" position="after">
                    <field name="res_partner_ids" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>